                <h3>Title</h3>
                <%= f.text_field :aeroport_title, class: "form-control" %>
                <h3>Description</h3>
                <%= f.text_field :aeroport_description, class: "form-control" %>
                <h3>Н1</h3>
                <%= f.text_field :aeroport_h1, class: "form-control" %>
                <h3>Над автомобилями</h3>
                <%= f.text_field :aeroport_avto, class: "form-control" %>
                <h3>Цена в рабочее время</h3>
                <%= f.text_field :aeroport_price, class: "form-control" %>
                <h3>Цена в нерабочее время</h3>
                <%= f.text_field :aeroport_price_unwork, class: "form-control" %>
                <h3>Текст</h3>
                <%= f.cktext_area :aeroport_text, ckeditor: { language: 'ru'}, class: "form-control" %>
              </div>
              <div class="text-center">
                <%= f.submit "Сохранить", class: "btn btn-outline-success" %>
