<% title @text.zelenogradsk_title %>
<% description @text.zelenogradsk_description %>
<%= render 'shared/header', cached: true %>
<div class="block_price">
    <div class="wrapper">
        <span></span>
        <div class="text1">C нами ты всегда у руля!</div>
        <div class="text2"><b>Аренда автомобилей</b> в Калининграде</div>
        <div class="text3">Зеленоградск</div>
        <%= link_to prices_path(locale: I18n.locale), class: "ceny" do %>
            Цены на аренду
        <% end %>
        <div class="clear"></div>
    </div>
</div>
<div class="block2 var2">
    <div class="wrapper" id="search_cars">
        <div class="zagol" style="padding-top: 30px;"><h1>Аренда автомобилей в Зеленоградске</h1></div>
        <search_cars city="Калининград"></search_cars>
        <div class="clear"></div>
    </div>
</div>
<div class="content for_price">
    <div class="wrapper">
        <div class="bl_dostavka">
            <div class="zagol"><h2>ЦЕНЫ НА ДОСТАВКУ в Зеленоградск</h2></div>
            <table>
                <thead>
                    <tr>
                        <th>Пункт доставки / возврата</th>
                        <th><b><%= @text.dop_table_worktime %></b></th>
                        <th><b><%= @text.dop_table_endtime %></b></th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Зеленоградск</td>
                        <td><%= @text.zelenogradsk_price %></td>
                        <td><%= @text.zelenogradsk_price_unwork %></td>
                    </tr>
                </tbody>
            </table>
            <div class="kak_nayti">
                <%= @text.zelenogradsk_text.html_safe %>
            </div>
        </div>
        <div class="clear"></div>
    </div>
</div>
<div class="block4 var2">
    <div class="wrapper">
        <div class="zagol"><%= @text.zelenogradsk_avto %></div>
        <div class="spis_avto active" id="spis1">
            <%= render 'cars' %>
            <div class="clear"></div>
        </div>
        <div class="clear"></div>
    </div>
</div>
<%= render 'shared/footer', cached: true %>
